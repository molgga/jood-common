{
  "name": "jood-common",
  "version": "0.0.0",
  "scripts": {
    "bootstrap": "yarn install",
    "ng": "ng",
    "ng:dev": "ng serve dev",
    "packages:build": "ng build packages --prod",
    "packages:test": "ng test packages --code-coverage",
    "packages:test-watch": "ng test packages --code-coverage --watch=true",
    "packages:doc": "jsdoc -c jsdoc.json -r -d docs",
    "page:document-dist": "jsdoc -c jsdoc.json -r -d docs",
    "page:clear": "rm -rf docs",
    "page:expose": "npm run page:document-dist",
    "ci:test": "ng test packages --watch=false --code-coverage --browsers=ChromeHeadless",
    "ci:build": "npm run packages:build",
    "ci:deploy-before": "npm run page:clear && npm run page:expose && npm run ci:build"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.1.1",
    "@angular/common": "~11.1.1",
    "@angular/compiler": "~11.1.1",
    "@angular/core": "~11.1.1",
    "@angular/forms": "~11.1.1",
    "@angular/platform-browser": "~11.1.1",
    "@angular/platform-browser-dynamic": "~11.1.1",
    "@angular/router": "~11.1.1",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1100.5",
    "@angular-devkit/build-ng-packagr": "^0.1000.4",
    "@angular/cli": "^11.1.1",
    "@angular/compiler-cli": "~11.1.1",
    "@angular/language-service": "~11.1.1",
    "@jood/common": "0.0.10",
    "@jood/jsdoc-theme": "^0.1.14",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^8.9.5",
    "better-docs": "^1.4.7",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.2",
    "jsdoc": "3.6.4",
    "karma": "^5.1.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "^2.0.3",
    "karma-jasmine": "~3.3.1",
    "karma-jasmine-html-reporter": "^1.5.2",
    "minimist": ">=1.2.5",
    "ng-packagr": "^10.0.3",
    "protractor": "~7.0.0",
    "rxjs": "^6.6.3",
    "ts-node": "~8.10.2",
    "tsickle": "^0.39.1",
    "tslint": "~6.1.2",
    "typescript": "~4.1.0"
  },
  "keywords": [
    "javascript",
    "typescript",
    "angular",
    "utility",
    "array",
    "string",
    "number",
    "date"
  ]
}
